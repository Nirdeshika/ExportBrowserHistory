{
	"manifest_version" : 2,
	"name" : "Browser to Excel",
	"description" : "Exports browsing history from browser to MS Excel",
	"version" : "1.0",
	"author" : "Nirdeshika Polisetti",

	"browser_action" : {"default_icon" : "icon.png",
						"default_title" : "Browser to Excel exporter",
						"default_popup" : "popup.html"
						},
	"background" : {"scripts" : ["background.js"]},

	"permissions" : ["tabs","history","downloads"]
}